@model List<FA.LibraryManagement.Common.ViewModels.CategoryVM>

<div class="nav-item dropdown">
    <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Category</a>
    <div class="dropdown-menu m-0">
        @foreach (var category in Model)
        {
            <a class="dropdown-item" asp-controller="Category" asp-action="Index" asp-route-categoryId="@category.Id" asp-area="Member">@category.Name</a>
        }
    </div>
</div>
