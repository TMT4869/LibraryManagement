@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
    <link rel="icon" type="image/x-icon" href="favicon.ico">
    <link rel="icon" type="image/png" href="~/static/images/logo/favicon.png">
    <link rel="stylesheet" crossorigin href="~/compiled/css/app.css">
    <link rel="stylesheet" crossorigin href="~/compiled/css/error.css">
</head>

<body>
    <script src="~/static/js/initTheme.js"></script>
    <div id="error">
        <div class="error-page container">
            <div class="col-md-8 col-12 offset-md-2">
                <div class="text-center">
                    <img class="img-error" src="~/compiled/svg/error-500.svg" alt="System Error">
                    <h1 class="error-title">@ViewData["Title"]</h1>
                    <p class="fs-5 text-gray-600">
                        This account has been locked out, please try again later.
                    </p>
                    <a asp-area="Member" asp-controller="Home" asp-action="Index"
                       class="btn btn-lg btn-outline-primary mt-3">Go Home</a>
                </div>
            </div>
        </div>


    </div>
</body>

</html>
